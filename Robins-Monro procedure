          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 800x500 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Robbins-Monro setup\n",
        "theta_star = 3         # true root\n",
        "theta0 = 10            # starting point\n",
        "N = 50                 # number of iterations\n",
        "a = lambda n: 1/(n+1)  # step size a_n = 1/(n+1)\n",
        "\n",
        "# simulate noisy observations: z(theta) = theta - theta_star + noise\n",
        "np.random.seed(42)\n",
        "theta = np.zeros(N+1)\n",
        "theta[0] = theta0\n",
        "\n",
        "for n in range(1, N+1):\n",
        "    z = theta[n-1] - theta_star + np.random.normal(0, 1)  # noise with std=1\n",
        "    theta[n] = theta[n-1] - a(n-1) * z  # note: minus sign because f(theta) = theta - theta_star\n",
        "\n",
        "# plot\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.plot(range(N+1), theta, marker='o', label=r'$\\theta^{(N)}$')\n",
        "plt.axhline(theta_star, color='r', linestyle='--', label=r'$\\theta^*$')\n",
        "plt.xlabel('Iteration N')\n",
        "plt.ylabel(r'Estimate $\\theta^{(N)}$')\n",
        "plt.title('Robbins-Monro Convergence Starting at Î¸(0)=10')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()\n"
      ]
    }
  ]
}
